		if (country.equals("FR")) {
			queryCustomerNumber = OracleJdbcConnection
					.crmDatabaseSelectQuery(DBUtils.queryMaxCustomerNumberFrCustomer());

		} else if (country.equals("UK")) {
			queryCustomerNumber = OracleJdbcConnection
					.crmDatabaseSelectQuery(DBUtils.queryMaxCustomerNumberUKCustomer());
